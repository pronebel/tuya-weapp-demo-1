/**index.wxss**/
page {
  background-color: #f8f8f8;
}

.btn {
  background-color: #ff5a28;
  border-radius: 15px;
  color: #fff;
  font-size: 22px;
  line-height: 58px;
  text-align: center;
  margin-bottom: 50px;
}

.headerBar {
  width: 90%;
  padding: 0 5%;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}

.headerBar-title {
  width: 56px;
	height: 20px;
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #22242c;
}

.adDevBtn {
  height: 20px;
  width: 20px;
}

.containers {
  height: 90vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.devItem {
  margin-top: 18px;
  height: 40px;
  padding: 19px 24px;
  width: 291px;
  display: flex;
  align-items: center;
	background-color: #fff;
	box-shadow: 0px 11px 21px 0px 
		rgba(43, 43, 43, 0.1);
	border-radius: 8px;
}

.devItem-img {
  width: 51px;
  height: 40px;
}

.devItem-title {
  margin-left: 15px;
  width: 128px;
	height: 22px;
	font-family: PingFangSC-Semibold;
  font-size: 16px;
  flex: 1;
	color: #495054;
}