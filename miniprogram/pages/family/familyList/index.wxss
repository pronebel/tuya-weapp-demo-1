/* pages/family/familyList/index.wxss */
page {
  background-color: #f8f8f8;
}

.familyList {
  margin-top: 16px;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.familyList-item {
  width: 100%;
  height: 56px;
  display: flex;
  align-items: center;
  background-color: #fff;
}

.familyList-item-title {
  margin-left: 16px;
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #22242c;
}

.rightArrow {
  display: flex;
}

.rightArrow-arrow {
  display: inline-block;
  position: relative;
  width: 8px;
  height: 8px;
  margin-right: 20px;
  border: 1px solid #c8c8c8;
  border-width: 2px 2px 0 0;
  transform: rotate(45deg);
}

.addFamily {
  height: 56px;
  width: 100%;
  position: absolute;
  bottom: 0;
  line-height: 56px;
  text-align: center;
  font-family: PingFangSC-Semibold;
  font-size: 16px;
  background-color: #fff;
}